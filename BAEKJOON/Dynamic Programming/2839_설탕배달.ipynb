    "n = int(input()) # 설탕\n",
    "\n",
    "count = 0 # 봉지 수\n",
    "\n",
    "while n >= 0:\n",
    "    if n % 5 == 0: \n",
    "        count += n // 5 \n",
    "        print(count)\n",
    "        break\n",
    "    n -= 3 \n",
    "    count += 1 \n",
    "else:\n",
    "    print(-1)"
   ]
